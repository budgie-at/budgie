{
    "extends": "../../tsconfig.json",
    "compilerOptions": {
        "composite": true,
        "tsBuildInfoFile": "./node_modules/.cache/tsbuildinfo.json",
        "baseUrl": "./",
        "rootDir": "./src",
        "lib": [
            "esnext"
        ],
        "outDir": "dist/esm",
        "module": "esnext",
        "noEmit": false
    },
    "include": [
